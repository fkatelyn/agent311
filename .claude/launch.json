{
  "version": "0.0.1",
  "configurations": [
    {
      "name": "backend",
      "runtimeExecutable": "uv",
      "runtimeArgs": ["--directory", "agent311", "run", "uvicorn", "agent311.main:app", "--reload", "--host", "0.0.0.0", "--port", "8000"],
      "port": 8000
